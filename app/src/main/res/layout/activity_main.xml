<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="@string/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:onClick="button1Click"
            app:layout_constraintHorizontal_bias="0.551" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"/>
    <EditText
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/secondaryPressureAt05"
            app:layout_constraintStart_toEndOf="@+id/textView7" android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp" app:layout_constraintBottom_toBottomOf="@+id/textView7"/>
    <EditText
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/secondaryPressureAt15"
            android:autofillHints=""
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintStart_toEndOf="@+id/textView8" android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp"/>
    <TextView
            android:text="@string/e2"
            android:layout_width="30dp"
            android:layout_height="wrap_content" android:id="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/resultE2" app:layout_constraintEnd_toStartOf="@+id/resultE2"
            android:layout_marginRight="20dp" android:layout_marginEnd="20dp" android:textSize="24sp"/>
    <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content" android:id="@+id/resultI0"
            android:background="#00FFE1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="40dp" android:layout_marginEnd="40dp"
            app:layout_constraintTop_toBottomOf="@+id/resultE2" android:layout_marginTop="30dp"
            android:textSize="24sp"/>
    <TextView
            android:text="@string/i0"
            android:layout_width="30dp"
            android:layout_height="wrap_content" android:id="@+id/I0"
            app:layout_constraintEnd_toStartOf="@+id/resultI0"
            android:layout_marginRight="20dp" android:layout_marginEnd="20dp"
            app:layout_constraintTop_toTopOf="@+id/resultI0" android:textSize="24sp"/>
    <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content" android:id="@+id/resultE2"
            android:background="#00FFE1" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="40dp" android:layout_marginEnd="40dp"
            app:layout_constraintTop_toBottomOf="@+id/secondaryPressureAt05" android:textSize="24sp"/>
    <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content" android:id="@+id/resultE1"
            android:background="#00FFE1"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="40dp"
            android:layout_marginEnd="40dp" app:layout_constraintTop_toBottomOf="@+id/primaryPressureAt05"
            android:textSize="24sp"/>
    <TextView
            android:text="Primary pressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView14" android:layout_marginTop="30dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp"/>
    <TextView
            android:text="0.05 MPa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView5"
            android:layout_marginLeft="40dp" android:layout_marginStart="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12" android:layout_marginTop="40dp"/>
    <TextView
            android:text="@string/e1"
            android:layout_width="30dp"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            app:layout_constraintEnd_toStartOf="@+id/resultE1" app:layout_constraintTop_toTopOf="@+id/resultE1"
            android:layout_marginRight="20dp" android:layout_marginEnd="20dp" android:textSize="24sp"/>
    <TextView
            android:text="0.15 MPa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView6"
            android:layout_marginLeft="40dp" android:layout_marginStart="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginTop="40dp"/>
    <EditText
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/primaryPressureAt05"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_marginLeft="40dp" android:layout_marginStart="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView5"/>
    <EditText
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/primaryPressureAt15"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            android:layout_marginLeft="40dp" android:layout_marginStart="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView6"/>
    <TextView
            android:text="Secondary pressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            android:layout_marginTop="50dp" app:layout_constraintStart_toStartOf="@+id/textView6"/>
    <TextView
            android:text="0.15 MPa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginTop="40dp" app:layout_constraintStart_toStartOf="@+id/textView7"/>
    <TextView
            android:text="0.05 MPa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/textView13"/>
    <TextView
            android:text="VSS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/textView14" android:layout_marginTop="20dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="36sp"/>

</android.support.constraint.ConstraintLayout>